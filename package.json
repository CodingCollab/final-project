    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "popper.js": "^1.14.3",
    "react": "^16.4.1",
    "react-day-picker": "^7.1.9",
    "react-dom": "^16.4.1",
